<div id="search">
		    <button type="button" class="close" id="closeSearch">Ã—</button>
		    <form>
		        <input type="search" value="" placeholder="Escriba la palabra(s) clave" />		        
		        <button type="submit" class="page-scroll btn btn-xl catalogo">Buscar</button>
		    </form>
